{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "beeseen/bee-accordion",
  "version": "v2.0.0",
  "title": "Bee Accordion Gallery",
  "category": "widgets",
  "icon": "images-alt2",
  "description": "Hover expands one panel while others compress.",
  "supports": {
    "html": false
  },
  "textdomain": "beeseen",
  "attributes": {
    "images": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object"
      }
    },
    "gap": {
      "type": "number",
      "default": 10
    },
    "height": {
      "type": "number",
      "default": 360
    },
    "radius": {
      "type": "number",
      "default": 18
    },
    "expand": {
      "type": "number",
      "default": 5
    },
    "fit": {
      "type": "string",
      "default": "cover"
    },
    "showCaption": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
