{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "beeseen/bee-spin-wheel",
  "version": "v2.0.1",
  "title": "Bee Spin Wheel",
  "category": "widgets",
  "icon": "format-image",
  "description": "Interactive spin wheel to tease choices and guide user decisions.",
  "supports": { "html": false },
  "textdomain": "beeseen",
  "attributes": {
    "items": { "type": "array", "default": [] },
    "durationMs": { "type": "number", "default": 5200 },
    "minTurns": { "type": "number", "default": 6 },
    "autoplay": { "type": "boolean", "default": false },
    "openInNewTab": { "type": "boolean", "default": false },
		"spinLabel": {
  "type": "string",
  "default": "Try your luck"
}

  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
