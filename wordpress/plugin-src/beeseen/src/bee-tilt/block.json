{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "beeseen/bee-tilt",
  "version": "v2.0.0",
  "title": "Bee Tilt",
  "category": "widgets",
  "icon": "format-image",
  "description": "Cursor-driven tilt with optional soft glare highlight.",
  "supports": {
    "html": false
  },
  "textdomain": "beeseen",
  "attributes": {
    "image": {
      "type": "object",
      "default": null
    },
    "maxTilt": {
      "type": "number",
      "default": 10
    },
    "perspective": {
      "type": "number",
      "default": 800
    },
    "glare": {
      "type": "boolean",
      "default": true
    },
    "glareStrength": {
      "type": "number",
      "default": 0.25
    },
    "borderRadius": {
      "type": "number",
      "default": 16
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
