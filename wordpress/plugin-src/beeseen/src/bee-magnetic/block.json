{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "beeseen/bee-magnetic",
  "version": "v2.0.1",
  "title": "Bee Magnetic",
  "category": "widgets",
  "icon": "format-image",
  "description": "Subtle magnetic drift toward cursor with smooth easing.",
  "supports": {
    "html": false
  },
  "textdomain": "beeseen",
  "attributes": {
    "image": {
      "type": "object",
      "default": null
    },
    "strength": {
      "type": "number",
      "default": 18
    },
    "maxOffset": {
      "type": "number",
      "default": 28
    },
    "ease": {
      "type": "number",
      "default": 0.14
    },
    "lift": {
      "type": "number",
      "default": 0
    },
    "borderRadius": {
      "type": "number",
      "default": 16
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
