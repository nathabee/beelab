{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "beeseen/bee-reveal",
  "version": "v2.0.1",
  "title": "Bee Reveal",
  "category": "widgets",
  "icon": "format-image",
  "description": "Hover reveal mask follows cursor to reveal alternate image.",
  "supports": {
    "html": false
  },
  "textdomain": "beeseen",
  "attributes": {
    "baseImage": {
      "type": "object",
      "default": null
    },
    "revealImage": {
      "type": "object",
      "default": null
    },
    "radius": {
      "type": "number",
      "default": 120
    },
    "feather": {
      "type": "number",
      "default": 60
    },
    "ease": {
      "type": "number",
      "default": 0.18
    },
    "borderRadius": {
      "type": "number",
      "default": 16
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
